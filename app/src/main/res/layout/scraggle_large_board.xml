<?xml version="1.0" encoding="utf-8"?>
<GridLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context="edu.neu.madcourse.binodthapachhetry.Scraggle.ScraggleGameActivity">

    <include android:id="@+id/large1" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="0" android:layout_row="0"/>
    <include android:id="@+id/large2" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="1" android:layout_row="0"/>
    <include android:id="@+id/large3" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="2" android:layout_row="0"/>
    <include android:id="@+id/large4" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="0" android:layout_row="1"/>
    <include android:id="@+id/large5" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="1" android:layout_row="1"/>
    <include android:id="@+id/large6" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="2" android:layout_row="1"/>
    <include android:id="@+id/large7" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="0" android:layout_row="2"/>
    <include android:id="@+id/large8" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="1" android:layout_row="2"/>
    <include android:id="@+id/large9" layout="@layout/scraggle_small_board"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_margin="@dimen/tictactoe6_small_board_margin"
        android:layout_column="2" android:layout_row="2"/>
</GridLayout>